<?xml version="1.0" encoding="UTF-8"?>
<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 https://maven.apache.org/xsd/maven-4.0.0.xsd">
    <modelVersion>4.0.0</modelVersion>
    <groupId>cn.caber</groupId>
    <artifactId>caber-app</artifactId>
    <version>${revision}</version>
    <modules>
        <module>caber-app-api</module>
        <module>caber-app-server</module>
        <module>caber-app-web</module>
        <module>caber-app-common</module>
    </modules>
    <packaging>pom</packaging>
    <properties>
        <java.version>8</java.version>
        <revision>1.0.0-SNAPSHOT</revision>
        <springboot.version>2.3.4.RELEASE</springboot.version>
        <dubbo.version>2.7.18</dubbo.version>
        <mybatis.version>3.5.6</mybatis.version>
        <spring.version>5.2.9.RELEASE</spring.version>
        <mybatisplus.version>3.4.1</mybatisplus.version>
        <zookeeper.version>3.4.6</zookeeper.version>
        <curator.version>2.12.0</curator.version>
        <spring.cloud.version>2.2.3.RELEASE</spring.cloud.version>
        <mapstruct.version>1.5.3.Final</mapstruct.version>
        <jwt.version>0.9.1</jwt.version>
        <jedis>3.3.0</jedis>
